<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="?android:selectableItemBackground"
    android:paddingStart="16dp"
    android:paddingTop="9dp"
    android:paddingEnd="16dp"
    android:paddingBottom="9dp">

    <ImageView
        android:id="@+id/iv_data_source_2_icon"
        android:layout_width="36dp"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:importantForAccessibility="no"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/ic_insert_drive_file_main_color_2_24_skin" />

    <com.skyd.imomoe.view.component.textview.TypefaceTextView
        android:id="@+id/tv_data_source_2_name"
        android:layout_width="0px"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="10dp"
        android:layout_weight="1"
        android:ellipsize="marquee"
        android:focusable="true"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:textColor="@color/foreground_main_color_2_skin"
        android:textSize="17sp"
        app:layout_constraintStart_toEndOf="@id/iv_data_source_2_icon"
        app:layout_constraintTop_toTopOf="parent"
        app:typeface="bPRTypeface"
        tools:text="CustomDataSource.jar" />

    <com.skyd.imomoe.view.component.textview.TypefaceTextView
        android:id="@+id/tv_data_source_2_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:ellipsize="marquee"
        android:focusable="true"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:textColor="@color/main_color_skin"
        app:layout_constraintStart_toStartOf="@id/tv_data_source_2_name"
        app:layout_constraintTop_toBottomOf="@id/tv_data_source_2_name"
        app:typeface="bPRTypeface"
        tools:text="1.1.0(1)" />

    <com.skyd.imomoe.view.component.textview.TypefaceTextView
        android:id="@+id/tv_data_source_2_author"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="12dp"
        android:ellipsize="marquee"
        android:focusable="true"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:textColor="@color/main_color_skin"
        app:layout_constraintStart_toEndOf="@id/tv_data_source_2_version"
        app:layout_constraintTop_toBottomOf="@id/tv_data_source_2_name"
        app:typeface="bPRTypeface"
        tools:text="author" />

    <com.skyd.imomoe.view.component.textview.TypefaceTextView
        android:id="@+id/tv_data_source_2_publish_at"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="12dp"
        android:ellipsize="marquee"
        android:focusable="true"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:textColor="@color/main_color_skin"
        app:layout_constraintStart_toEndOf="@id/tv_data_source_2_author"
        app:layout_constraintTop_toBottomOf="@id/tv_data_source_2_name"
        app:typeface="bPRTypeface"
        tools:text="2022-02-21 21:20" />

    <com.skyd.imomoe.view.component.textview.TypefaceTextView
        android:id="@+id/tv_data_source_2_describe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:ellipsize="marquee"
        android:focusable="true"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:textColor="@color/main_color_skin"
        app:layout_constraintStart_toStartOf="@id/tv_data_source_2_name"
        app:layout_constraintTop_toBottomOf="@id/tv_data_source_2_version"
        app:typeface="bPRTypeface"
        tools:text="描述" />
</androidx.constraintlayout.widget.ConstraintLayout>